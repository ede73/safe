<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   https://developer.android.com/identity/data/autobackup
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <include
            domain="database"
            path="safe-journal" />
        <include
            domain="database"
            path="safe" />
        <include
            domain="sharedpref"
            path="fi.iki.ede.safe.debug_preferences.xml" />
    </cloud-backup>
    <device-transfer>
        <include
            domain="database"
            path="safe-journal" />
        <include
            domain="database"
            path="safe" />
        <include
            domain="sharedpref"
            path="fi.iki.ede.safe.debug_preferences.xml" />
    </device-transfer>
</data-extraction-rules>